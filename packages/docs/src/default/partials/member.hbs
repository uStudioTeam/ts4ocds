<section class="tsd-panel tsd-member {{cssClasses}}">
  {{#if name}}
    <h3>
      <a id="{{anchor}}" class="tsd-anchor"></a>

      {{{wbr name}}}

      {{#each flags}}
        <span class="tsd-flag ts-flag{{this}}">
          {{this}}
        </span>
      {{/each}}
    </h3>
  {{/if}}

  {{#if signatures}}
    {{> member.signatures}}
  {{else}}{{#if hasGetterOrSetter}}
    {{> member.getterSetter}}
  {{else}}{{#if isReference}}
    {{> member.reference}}
  {{else}}
    {{> member.declaration}}
  {{/if}}{{/if}}{{/if}}

  {{#each groups}}
    {{#each children}}
      {{#unless hasOwnDocument}}
        {{> member}}
      {{/unless}}
    {{/each}}
  {{/each}}
</section>
